{
  "manifest_version": 3,
  "name": "YouTube Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },
  "permissions": ["tabs", "activeTab"],
  "host_permissions": ["https://www.youtube.com/*"],
  "background": {
    "service_worker": "popup.js",
    "type": "module"
  }
}
